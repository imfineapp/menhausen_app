{
  "name": "menhausen-telegram-mini-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview --host",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:css": "stylelint styles/*.css --max-warnings 0",
    "lint:all": "npm run lint && npm run lint:css",
    "type-check": "tsc --noEmit",
    "test": "NODE_NO_WARNINGS=1 vitest",
    "test:ui": "NODE_NO_WARNINGS=1 vitest --ui",
    "test:run": "NODE_NO_WARNINGS=1 vitest run",
    "test:coverage": "NODE_NO_WARNINGS=1 vitest run --coverage",
    "test:watch": "NODE_NO_WARNINGS=1 vitest --watch",
    "test:e2e": "NODE_NO_WARNINGS=1 playwright test",
    "test:e2e:ui": "NODE_NO_WARNINGS=1 playwright test --ui",
    "test:e2e:headed": "NODE_NO_WARNINGS=1 playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "test:all": "npm run test:run && npm run test:e2e",
    "playwright:install": "playwright install"
  },
